<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None" />
  </appSettings>
  <system.webServer>
    <!--This Modules part fixes the SQL error from checking user roles-->
    <modules runAllManagedModulesForAllRequests="true">
      <remove name="RoleManager" />
    </modules>
    <!--Leave this or it erases the place where roles are saved -->
    <validation validateIntegratedModeConfiguration="false" />
    <handlers>
      <add name="CaptchaImage" verb="GET" path="CaptchaImage.axd" type="MSCaptcha.CaptchaImageHandler, MSCaptcha" />
    </handlers>
  </system.webServer>
  <system.web>
    <roleManager enabled="true" cacheRolesInCookie="true"></roleManager>
    <authentication mode="Forms">
      <forms loginUrl="~/Member/MemberLogin.aspx" protection="All" timeout="30" name="AppNameCookie" path="/FormsAuth" requireSSL="false" slidingExpiration="true" defaultUrl="~/Root/Default.aspx" cookieless="UseCookies" enableCrossAppRedirects="false" />
    </authentication>
    <authorization>
      <deny users="?" />
    </authorization>
    <httpHandlers>
      <add verb="GET" path="CaptchaImage.axd" type="MSCaptcha.CaptchaImageHandler, MSCaptcha" />
    </httpHandlers>
    <customErrors mode="Off" />
    <compilation targetFramework="4.6.1" />
    <httpRuntime targetFramework="4.6.1" />
  </system.web>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IService1" />
        <binding name="BasicHttpBinding_IService11" />
        <binding name="BasicHttpBinding_IService" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://webstrar47.fulton.asu.edu/Page1/Service1.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IService1" contract="MMServiceReference1.IService1" name="BasicHttpBinding_IService1" />
      <endpoint address="http://webstrar47.fulton.asu.edu/Page8/Service1.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IService11" contract="DiyaloReference1.IService1" name="BasicHttpBinding_IService11" />
      <endpoint address="http://webstrar47.fulton.asu.edu/Page4/Service.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IService" contract="MarcusReference1.IService" name="BasicHttpBinding_IService" />
    </client>
  </system.serviceModel>
</configuration>
